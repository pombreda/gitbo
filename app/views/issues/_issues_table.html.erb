<div class="table_div">
  <table id="issues_table" cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" data-source="<%= issues_url(format: "json") %>" >
    <thead>
      <tr> 
        <th></th>
        <th>Issue Name</th>
        <th>Repo Name/ #Issue</th>
        <th>Comment Count</th>
        <th>Popularity</th>        
        <th>Bounty</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <%= render 'issues/issues-data' %>
    </tbody>
  </table>
</div>
